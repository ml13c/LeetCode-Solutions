class Solution {
public:
    int numSubmat(vector<vector<int>>& mat) {
        int counter1 = 0;
        int count2 = 0;
        for(int i = 0;i<m;i++){
            for(int j = 0;j<n;j++){
                    if(mat[i][j]==0){
                        nums[j]=0;
                    }
                    else{
                        nums[j] = nums[j-1]+1;
                    }
            }
        }
        return (counter1 + count2);
    }
};
